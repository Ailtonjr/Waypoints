    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="br.com.waypoints.waypoints.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="br.com.waypoints.waypoints.CadastroActivity" />
        <activity android:name="br.com.waypoints.waypoints.ConfigActivity" />
        <activity android:name="br.com.waypoints.waypoints.PrincipalActivity" />
        <activity android:name="br.com.waypoints.waypoints.RotasActivity" />
        <activity android:name="br.com.waypoints.waypoints.SobreActivity" />
        <activity android:name="br.com.waypoints.waypoints.RecuperarActivity" />
        <activity
            android:name="br.com.waypoints.waypoints.MenuActivity"
            android:label="@string/title_activity_menu"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>