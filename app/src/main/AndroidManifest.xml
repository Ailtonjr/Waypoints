<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.waypoints.waypoints">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CadastroActivity"
            android:label="@string/string_cadastro" />
        <activity
            android:name=".SobreActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RecuperarActivity"
            android:label="@string/string_recuperaSenha" />
        <activity
            android:name=".RotasActivity"
            android:label="@string/string_rotas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".GruposActivity"
            android:label="@string/string_grupos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ConfigActivity"
            android:label="@string/string_configuracoes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NovaRotaActivity" />
        <activity android:name=".AdicionaPontoActivity" />
        <activity android:name=".AdicionaGrupoActivity"></activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>